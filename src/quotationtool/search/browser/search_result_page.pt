<metal:block define-macro="search-result-page">
<div i18n:domain="quotationtool">

  <metal:block define-slot="label">
    <h1 i18n:translate="search-result-label">Search Result</h1>
  </metal:block>

  <div id="query-history">
    <div class="this-query">
      <span i18n:translate="search-query">
	Search query:
      </span>
      <span tal:content="view/query"/>
    </div>
  </div>

  <metal:block define-slot="result-rows">
    PLEASE
  </metal:block>

</div>
</metal:block>